alias orderExecutorIc d0

alias order r0
alias executorOkupansi r1
alias address r2

s db Setting 0
put db 0 0

Loop:
l order db Setting
bgtal order 0 PlaceOrder
yield
j Loop

PlaceOrder:
yield
l executorOkupansi orderExecutorIc Setting
brgt executorOkupansi 0 -2
get address db 0
put orderExecutorIc address order
add address address 1
put db 0 address
s db Setting 0
j ra